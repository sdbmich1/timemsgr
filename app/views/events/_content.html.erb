<div class="span-24 last" id="event_content">  	<% if @form == "event_slider" %>		<div id="events"><%= render 'showtime' %></div>			<div class="nav-right timeframe"><%= render "dd_daterange" %></div>	<% else %>		<center><h3><%= set_header(@form) %></h3></center> 	<% end %>	<hr />  	<% if !notice.nil? %>  	   <p class="notice"><%= notice %></p>  	<% end %>	<div id="event_form"><%= render :partial => @form %></div> 	</div>