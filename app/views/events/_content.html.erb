<div class="span-24 last" id="event_content">  	<% if @form == "event_slider" %>		<div id="events"><%= render 'showtime' %></div>		<div class="rpos">				<span class="timeframe"><%= render "dd_daterange" %></span>		</div>	<% else %>		<center><h3><%= set_header(@form) %></h3></center> 	<% end %>	<hr />	<div class="span-21 last prepend-5">  		<% unless notice.nil? %>  	   		<p class="notice"><%= notice %></p>  		<% end %>  	</div>	<div id="event_form"><%= render :partial => @form %></div> 	</div>