<div class="span-1"><span class="grphead"><%= f %></span></div><div class="span-2 slider-nav">	<%= image_tag "rsz_left_arrow_nav.png", :class => "#{set_slider_class(f)[:lnav]} adj-left" %> </div><div class="span-19">	<div class="#{set_slider_class(f)[:stype]}">		<ul class="eslide">			<% if f == "Observances"%>				<%= render :partial => 'event', :as => 'event', :collection => @events.select {|event| event.event_type == "obsrv"} %>			<% elsif f == "Opportunities" %>				<%= render @events %>			<% else %>				<%= render :partial => 'event', :as => 'event', :collection => @events.select {|event| event.user_id == @user.id} %>			<% end %>		</ul>	</div></div><div class="span-2 last">	<%= image_tag "rsz_right_arrow_nav.png", :class => "#{set_slider_class(f)[:rnav]} slider-nav" %></div>