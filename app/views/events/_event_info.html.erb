<div class="span-5 event-label">   	<p><%= f.label "What are you planning?" %></p>           </div>	<div class="span-10" id="new-event">  	<span>Event Title </span><%= f.text_field :event_title, :size => 40 %></div><div class="span-9 last" id="new-event">  	<span>Frequency </span><%= f.collection_select :reoccurrencetype, ReoccurrenceType.all, :code, :description, :include_blank => true %></div><div class="span-19 last" id="new-event">	<div id="eventtype">		<%= f.hidden_field :activity_type %>		<%= f.hidden_field :event_type %>				<%= render :partial => 'typelist', :locals => { :typelist => @options, :f => f } %>
	</div></div><hr />