<div class="span-4"> 	<%= get_start_date(event.start_date, event.end_date, "List") %><br />	<%= " #{event.start_time.strftime("%l:%M %p")} - #{event.end_time.strftime("%l:%M %p")}" %>			</div><div class="span-2" id="new-event">	<%= image_tag event.photo.url(), :alt => "none", :size => "30x30" %></div><div class="span-12">	<%= link_to event.title, event_url(event), :remote => true %></div><div class="span-2">	<% if chk_start_dt(event.start_date) %>		<%= link_to 'Edit', edit_event_url(event) %>	<% else %>		<span class="edit-event">Edit</span> 	<% end %></div><div class="span-2">	<%= link_to 'Clone', clone_event_url(event) %></div><div class="span-2 last">	<p><%= link_to "Delete", event, :method => :delete, :remote => true %></p></div> 