<div class="span-4"> 	<%= get_start_date(event.eventstartdate, event.eventenddate, "List") %><br />	<%= " #{event.eventstarttime.strftime("%l:%M %p")} - #{event.eventendtime.strftime("%l:%M %p")}" %>			</div><div class="span-14">	<%= link_to event.event_title, event_url(event), :class => "show-item" %></div><div class="span-2">	<% if chk_start_dt(event.eventstartdate) %>		<%= link_to 'Edit', edit_event_url(event) %>	<% else %>		<span class="edit-event">Edit</span> 	<% end %></div><div class="span-2">	<%= link_to 'Clone', clone_event_url(event), :confirm => "Clone this event?" %></div><div class="span-2 last">	<p><%= link_to "Delete", event, :confirm => "Are you sure?", :method => :delete, :remote => true %></p></div> 