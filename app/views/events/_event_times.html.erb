<div class="span-5 event-label">  	<p><%= f.label "When does it begin?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<div class="span-6">		<%= f.label :start_date %><%= f.text_field :start_date, :size => 10, :id => "start-date" %> 	</div>	<div class="span-13 last">		<%= f.label :start_time %><%= f.text_field :start_time, :size => 10, :id => "start-time" %> 	</div>	<p><%= f.label :start_time_zone %><%= f.time_zone_select :start_time_zone, /US/ %></p></div><hr />   <div class="span-5 event-label">  	<p><%= f.label "When does it end?" %></p>                     </div>	<div class="span-19 last" id="new-event">  	<div class="span-6">		<%= f.label :end_date %><%= f.text_field :end_date, :size => 10, :id => "end-date" %> 	</div>	<div class="span-13 last">		<%= f.label :end_time %><%= f.text_field :end_time, :id => "end-time", :size => 10 %> 	</div>	<p><%= f.label :end_time_zone %><%= f.time_zone_select :end_time_zone, /US/ %></p></div><hr />  