<div class="span-5 event-label">  	<p><%= f.label "When does it begin?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<div class="span-9">		<span>Start Date </span><%= f.text_field :eventstartdate, :size => 10, :id => "start-date" %> 	</div>	<div class="span-10 last">		<span>Start Time </span><%= f.text_field :eventstarttime, :value => chk_time(@event.eventstarttime), :id => "start-time", :size => 10 %> 	</div>	<p><span>Start Time Zone </span><%= f.collection_select :localGMToffset, GMTTimezone.all, :code, :description, :include_blank => true %></p></div><hr />   <div class="span-5 event-label">  	<p><%= f.label "When does it end?" %></p>                     </div>	<div class="span-19 last" id="new-event">  	<div class="span-9">		<span>End Date </span><%= f.text_field :eventenddate, :size => 10, :id => "end-date" %> 	</div>	<div class="span-10 last">		<span>End Time </span><%= f.text_field :eventendtime, :value => chk_time(@event.eventendtime), :id => "end-time", :size => 10 %> 	</div>	<p><span>End Time Zone </span><%= f.collection_select :endGMToffset, GMTTimezone.all, :code, :description, :include_blank => true %></p></div><hr />  