<center><span class="grphead"><%= "#{slider}" %></span></center><div class="span-21 last prepend-5">	<%= image_tag "#{nav_btn_img('left')}", :class => "#{set_slider_class(slider)[:lnav]} left-nav slider-nav" %> 	<%= image_tag "#{nav_btn_img('right')}", :class => "#{set_slider_class(slider)[:rnav]} right-nav slider-nav" %> 	<% if !(slider =~ /Nearby/i).nil? %>		<div class="span-19 last">			<div class="rpos">					<span class="loc-hdr"><%= render "shared/locations" %></span>  			</div>		</div>		<div class="opp-slider">			<ul class="eslide">				<% get_date_range(@nearby_events, @enddate).each do |edate| %>					<%= render :partial => 'event', :collection => @nearby_events, :as => :event, :locals => {:slider => slider, :edate => edate} %>					<% end %>				</ul>		</div>	<% elsif !(slider =~ /Suggested/i).nil? %>		<div class="span-19 last">			<div class="btn-top">				<%= link_to "Manage", subscriptions_url(:id=>@user), :class=>"edit-btn round nav-right" %>			</div>		</div>		<div class="sub-slider">			<ul class="eslide">				<% get_date_range(@events, @enddate).each do |edate| %>					<%= render :partial => 'event', :collection => get_events(slider, edate),:as => :event, :locals => {:slider => slider, :edate => edate } %>				<% end %>			</ul>		</div>			<% else %>		<div class="span-19 last">			<div class="btn-top">				<%= link_to "Manage", private_events_url, :class=>"edit-btn round nav-right" %>			</div>		</div>		<div class="sch-slider">			<ul class="eslide">				<%= render :partial => 'load_schedule', :locals => {:slider => slider, :edate => @enddate } %>						</ul>	    </div>			<% end %></div>