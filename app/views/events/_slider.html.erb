<center><span class="grphead"><%= "#{slider}" %></span></center><div class="span-21 last prepend-5">	<%= image_tag "#{nav_btn_img('left')}", :class => "#{set_slider_class(slider)[:lnav]} left-nav slider-nav" %> 	<%= image_tag "#{nav_btn_img('right')}", :class => "#{set_slider_class(slider)[:rnav]} right-nav slider-nav" %> 	<% if !(slider =~ /Upcoming/i).nil? %>		<div class="opp-slider">			<ul class="eslide"><%= render :partial => 'event', :collection => get_slider_events(slider), 				:as => :event, :locals => {:slider => slider } %></ul>		</div>	<% elsif !(slider =~ /Observances/i).nil? %>		<div class="obsrv-slider">			<ul class="eslide"><%= render :partial => 'event', :collection => get_slider_events(slider),			  	:as => :event, :locals => {:slider => slider } %></ul>		</div>			<% else %>		<div class="sch-slider">			<ul class="eslide"><%= render :partial => 'event', :collection => get_slider_events(slider),				:as => :event, :locals => {:slider => slider } %></ul>	    </div>			<% end %></div>