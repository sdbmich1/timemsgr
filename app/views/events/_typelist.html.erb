<%= label_tag "Event Type" %>	<% if !typelist.blank? %>	<% debugger %>	<% if @event.event_type.blank? %>		<%= select_tag 'event_type', options_for_select(typelist.collect{ |c|[c.Description, c.Code] }, {:prompt => "--Select Type--"}) %>	<% else %>		<%= select_tag 'event_type', options_for_select(typelist.collect{ |c|[c.Description, c.Code] }, @event.event_type) %>	<% end %> <% end %>