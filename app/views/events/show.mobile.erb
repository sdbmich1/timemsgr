<%= content_for :pre_mobile_load do %>
	<%= render 'mobile/pre_load' %>
<% end %>

<%= content_for :head do %>
 	<%= link_to 'Home', root_path, "class"=>"ui-btn-left", "data-icon"=>"home", "data-iconpos"=>"notext" %>
<% end -%>

<%= content_for :content do %>
  <div id="show-item">
  	<% if tsd_event?(@event.event_type) %>
		<%= render :partial => 'mobile/show_major_event', :locals => {:event => @event} %>
  	<% else %>  
		<%= render :partial => 'mobile/show_event', :locals => {:event => @event, :session_flg => 'No'} %>
	<% end %> 	  
  </div>
<% end %>	
