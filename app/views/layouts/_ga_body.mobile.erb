<script>
$('[data-role=page]').live('pageshow', function (event, ui) {
    try {
   		_gaq.push(['_setAccount', 'UA-28841665-1']);
        hash = location.hash;

        if (hash) {
            _gaq.push(['_trackPageview', hash.substr(1)]);
        } else {
            _gaq.push(['_trackPageview']);
        }
      } catch(err) {

    }
});
	
</script>
