<!DOCTYPE html>
<html>
  <head>  
  	<%= yield :title %> 
	<%= render 'layouts/jqm_datebox' %>
    <%= stylesheet_link_tag "mobile", "calendar", :cache => true %>  
    <% if !user_signed_in? %>
    	<%= stylesheet_link_tag "mobile_device" %>
    <% end %>
  	<%= javascript_include_tag 'application.mobile.js','jquery.infieldlabel.min.js', "autocomplete-rails.js", :cache => true %>
  	<%= csrf_meta_tag %>  	
  </head>
  <body>  
  <div id="formapp" data-role="page"> 
    <div data-role="header" data-theme="b">
      <%= yield :head %>
    </div>
    
    <div id="banner_ad_top">
    	<div class="center-wrapper">
    		<%= yield :ad_content %>
    	</div>
    </div>
    
	<div class="app-content" data-role="content">
		<%= yield :subhead %>
      	<%= yield :content %>
    </div>
    
    <div id="app-footer" data-role="footer">		
      <%= yield :footer %>
    </div>
   </div>   
     
  </body> 
</html>
