<!DOCTYPE html>
<html>
  <head>  
  	<%= yield :title %> 
	<%= render 'layouts/jqm' %>
   	<%= stylesheet_link_tag "mobile", "calendar", :cache => true %>  
   	<%= stylesheet_link_tag "mobile_device" unless user_signed_in? %>
  	<%= csrf_meta_tag %>  	
  </head>
  <body>  
  <div id="mapPage" data-role="page" data-cache="never"> 
    <div data-role="header" data-theme="b">
      <%= yield :head %>
    </div>
    
    <div id="banner_ad_top">
    	<div class="center-wrapper">
    		<%= yield :ad_content %>
    	</div>
    </div>
    
	<div id='map_content' data-role="content">
		<%= yield :subhead %>
      	<%= yield :content %>
    </div>
    
    <div id="app-footer" data-role="footer">		
      <%= yield :footer %>
    </div>
   </div>   
     
  </body> 
</html>
