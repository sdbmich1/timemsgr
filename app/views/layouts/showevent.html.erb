<!DOCTYPE html>
<html>
  <head>	
   	<%= stylesheet_link_tag 'custom', :media => 'screen' %>     		
 	<%= render 'shared/initialize' %>	  	
  	<%= javascript_include_tag "https://js.stripe.com/v1/","http://maps.google.com/maps/api/js?sensor=true",'map.js'  %>
	<%= stylesheet_link_tag 'layout', 'links','jquery.fancybox-1.3.4', :media => 'screen' %>
  	<%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
 	<%= yield :head %>
  </head>
  <body>
 	<div class="container landing-bg" id="landing">  
 		<div id="spinner" style="display:none"></div>
 		<%= yield :left_sidebar %>
	  	<%= yield :content %>
		<%= yield :scripts %>
	</div>
	<div id="footer" class="container">
		<%= yield :footer %>		
	</div>
	<%= render 'shared/jsheets'%>
    <%= render :partial => 'layouts/ga' if Rails.env.production? %>      
  </body>
</html>
