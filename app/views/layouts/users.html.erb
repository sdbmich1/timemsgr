<!DOCTYPE html>
<html>
  <head>	
   		<%= stylesheet_link_tag 'custom', :media => 'screen', :cache => true %>     		
 	  	<%= render 'shared/initialize' %>

   		<% if user_signed_in? %>
   			<%= javascript_include_tag "autocomplete-rails.js",'menu.js', :cache => true %>
   		<% end %>
		<%= stylesheet_link_tag 'layout','screen','users','style', :media => 'screen', :cache => true %> 
 	  	<%= yield :head %>
  </head>
  <body>
	<div class="container landing-bg" id="landing">  
		<%= yield :subhead %>
 	  	<div id="flash_notice" style="display:none"></div>
  		<div id="spinner" style="display:none"></div>
	  	<%= yield :left_sidebar %>
	  	<%= yield :content %>	  	
		<%= render 'layouts/footer' %>
	</div> 
 </body>
</html>
