  <% if has_facebook_photo? && model.pictures.blank? %>
  	<% if controller_name == 'users' && action_name != 'index' %>
   		<%= image_tag @facebook_user.picture, :size => psize %> 
   	<% else %>
   		<%= image_tag file_name, :alt => "none", :size => psize %>
   	<% end %> 
  <% elsif !(model.class.to_s =~ /Event/i).nil? && image_exists?(model) %>
   	<%= image_tag model.imagelink.html_safe, :size => psize, :class => vtype rescue nil %>  	
  <% elsif model.try(:pictures).blank? %>
    <%= image_tag file_name, :alt => "none", :size => psize %>
  <% end %>