<script type="text/javascript">	$(document).bind("mobileinit", function(){		// set ajax to false		$.extend(  $.mobile, { ajaxFormsEnabled: false });		 		// keeps my page transitions from "jumping"  		$.extend($.mobile, {			metaViewportContent: "width=device-width, minimum-scale=1, maximum-scale=1"		});				//reset type=date inputs to text    	$.mobile.page.prototype.options.degradeInputs.date = true;	 });	 	 </script>