<%= content_for :head do %>
	<div data-role="navbar">
	  	<ul>
			<li><%= link_to "All", relationships_url(:uid=>@user, :ctype => 'All'), "data-theme"=>"#{theme[0]}"%> </li>
			<li><%= link_to "Private", private_relationships_url(:uid=>@user, :ctype => 'Private'), "data-theme"=>theme[1] %> </li>
			<li><%= link_to "Social", social_relationships_url(:uid=>@user, :ctype => 'Social'), "data-theme"=>theme[2] %> </li>
		</ul>
	</div>
<% end %>

<%= content_for :content do %>
	<%= render :partial => 'mobile/user_list', :locals => {:model => @trackers}%>
<% end %>

<%= content_for :footer do %>
	<% if rel_type != 'All' %>
	  <div data-role="navbar">
	  	<ul>
			<li><%= link_to "Add", users_url(:uid=>@user, :rel_type=>rel_type), "data-icon"=>"plus", :method => "post" %> </li>
		</ul>
	  </div>
	<% end %>
<% end %>
