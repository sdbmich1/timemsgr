<%= form_tag searches_path, :method => :get do %>   <div id="search-mform" class="center-wrapper">	<%= text_field_tag 'search', nil, :placeholder => 'Enter search term...', :class=>'search-fld' %>	<%= submit_tag 'Search', "data-theme"=>"b"  %>	<%= link_to "Cancel", home_url, "data-role" => "button" %>  </div><% end %>