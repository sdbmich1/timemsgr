<div class="interest">
	<% @subscriptions.each do |sub| %>		
		<fieldset data-role="controlgroup"> 
			<%= check_box_tag :subscription_ids, sub.id, true, :name => 'user[subscription_ids][]' -%>
			<label for="subscription_ids"><%= sub.channel_name %></label>
      	</fieldset> 			
	<% end %>
</div>
