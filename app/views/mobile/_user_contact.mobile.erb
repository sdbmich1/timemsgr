<%= f.fields_for :host_profiles do |form| %>  	<p><span>Address 1 </span><%= form.text_field :Address1 %></p>	<p><span>Address 2 </span><%= form.text_field :Address2 %></p>	<p><span>City </span><%= form.text_field :City %></p>	<p><span>State/Province </span>		<%= form.collection_select :State, StateCode.all, :StateAbbr, :State, :prompt => true %></p>	<p><span>Zip </span><%= form.text_field :PostalCode, :size => 10 %></p>	<p><span>Country </span><%= form.country_select :Country, ["United States"], :prompt => true %></p>	  	<p><span>Home Phone </span><%= form.telephone_field :Phone_Home %></p>	<p><span>Work Phone </span><%= form.telephone_field :Phone_Work %></p>	<p><span>Cell Phone </span><%= form.telephone_field :Phone_cell %></p>	<p><span>Wireless Provider </span><%= form.text_field :wirelessservice %></p><% end %> 