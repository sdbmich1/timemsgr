<%= f.fields_for :host_profiles do |form| %> 	<%= render :partial => 'mobile/user_address', :locals => {:form => form, :toggle => true} %>	<p><label>Work Phone </label><%= form.telephone_field :Phone_Work %></p>	<p><label>Cell Phone </label><%= form.telephone_field :Phone_cell %></p>	<p><label>Wireless Provider  </label>		<%= form.collection_select :wirelessservice, WirelessProvider.unhidden, :code, :description, {:prompt => 'Select Provider'} %></p><% end %> 