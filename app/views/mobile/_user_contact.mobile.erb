<%= f.fields_for :host_profiles do |form| %>  	<p><label>Address 1 </label><%= form.text_field :Address1, 'data-mini'=>'true' %></p>	<p><label>Address 2 </label><%= form.text_field :Address2, 'data-mini'=>'true' %></p>	<p><label>City </label><%= form.text_field :City, 'data-mini'=>'true'%></p>	<p><label>State/Province </label>		<%= form.collection_select :State, StateCode.all, :StateAbbr, :State, {:prompt => 'Select State'}, {'data-mini'=>'true'} %></p>	<p><label>Zip </label><%= form.text_field :PostalCode, :size => 10, 'data-mini'=>'true' %></p>	<p><label>Country </label><%= form.country_select :Country, ["United States"], {}, {'data-mini'=>'true'} %></p>	  	<p><label>Home Phone </label><%= form.telephone_field :Phone_Home, 'data-mini'=>'true' %></p>	<p><label>Work Phone </label><%= form.telephone_field :Phone_Work, 'data-mini'=>'true' %></p>	<p><label>Cell Phone </label><%= form.telephone_field :Phone_cell, 'data-mini'=>'true' %></p>	<p><label>Wireless Provider </label><%= form.text_field :wirelessservice, 'data-mini'=>'true' %></p><% end %> 