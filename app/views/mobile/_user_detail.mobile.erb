<% unless controller_name == 'users' %>  	<%= render :partial => 'mobile/show_photo', :locals => {:model => model, :vtype => 'ppic', :file_name => 'headshot2.jpg', :psize=>'80x80' }%> <% else %>	<%= render :partial => 'mobile/show_photo', :locals => {:model => model.profile, :vtype => 'ppic', :file_name => "headshot2.jpg", :psize => '80x80' } %>		  <% end %>
<div class="pstr">	<h6><%= "#{model.first_name} #{model.last_name}" %></h6></div><% if (controller_name =~ /users/i).nil? %>	<p class="circle_name"><%= get_status(@user.id, model.id) %>: <%= get_rel_type(@user.id, model.id) %> Circle</p><% end %>