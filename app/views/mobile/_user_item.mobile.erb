<li data-icon="<%= getIconType(@user.id, model.id) %>">	<% if getMethod(@user.id, model.id) == 'post' %>		<%= link_to relationships_url(:id=>@user, :trkr_id=>model, :rtype=>@rel_type), :method=>getMethod(@user.id, model.id), :confirm=>getConfirmMsg(@user.id, model.id), "data-ajax"=>"false" do %>				<%= render :partial => 'mobile/user_detail', :locals => {:model => model} %>		<% end %>		<% else %>		<%= link_to relationship_url(:id=>model, :trkr_id=>@user, :rtype=>@rel_type), :method=>getMethod(@user.id, model.id), :confirm=>getConfirmMsg(@user.id, model.id), "data-ajax"=>"false" do %>				<%= render :partial => 'mobile/user_detail', :locals => {:model => model} %>		<% end %>		<% end %>		</li>