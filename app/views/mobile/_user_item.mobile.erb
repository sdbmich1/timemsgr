	<li>		<% unless controller_name == 'users' %>			<%= link_to user_url(model) do %>				<%= render :partial => 'mobile/user_detail', :locals => {:model => model} %>			<% end %>		<% else %>			<%= link_to relationships_url(:trk_id=>@user.id, :rtype=>@rel_type), :method => :post, :remote=>true do %>					<%= render :partial => 'mobile/user_detail', :locals => {:model => model} %>			<% end %>		<% end %>	</li>