	<%= render :partial => 'mobile/user_name_email', :locals=>{:f=>f} %>	<tr><td>Gender </td><td><%= f.select :gender, options_for_select( ['Male', 'Female'] ) %></td></tr>	<tr>		<td>Birthdate</td>		<td>			<div data-role="fieldcontain">				<fieldset data-role="controlgroup" data-type="horizontal">				<%= f.date_select :birth_date, {:as => :date, :start_year => Date.today.year - 13, :end_year => Date.today.year - 90,                                 :order => [:month, :day, :year], :required => true, :prompt => true}, {'data-mini'=>'true'}  %>    			</fieldset>			</div> 		</td>	</tr>	<tr><td>Nearest City </td><td><%= f.collection_select :location_id, Location.all, :id, :city, {:prompt => 'Select City'} %></td></tr>            	