
<%= content_for :title do %>
  	<title><%= "#{get_name_or_logo}" %></title>
<% end -%>

<%= content_for :head do %>
	<% if user_signed_in? %>
   		<h1><%= "#{get_name_or_logo}" %></h1>
       	<%= link_to 'Account', home_user_path(current_user), "class" => "ui-btn-right", "data-icon"=>"gear", "data-iconpos"=>"notext" %>
    <% end %>
<% end -%>

<%= content_for :content do %>
  	<div class="mtop center-wrapper">
		<p><%= image_tag("#{get_name_or_logo('logo')}", :class =>"logo") %></p>
  	</div>
   	<%= render :partial => "devise/shared/signin" %><br />
 	<div class="mtop">
  		<div class="version nav-right"><%= "Build: #{get_version}" if Rails.env.production? %></div>
	</div>
<% end -%>

<%= content_for :footer do %>
	<%= render 'layouts/footer' %>
<% end %>

