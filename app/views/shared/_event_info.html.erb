<div class="span-5 event-label">   	<p><%= f.label "What are you planning?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<span>Name </span><%= f.text_field :event_name, :size => 60 %></div><div class="span-19 last" id="new-event">  <div id="eventtype">	<span>Event Type </span>	<% unless controller_name == 'life_events' %>		<%= f.collection_select :event_type, EventType.all, :code, :descr_title, :prompt => 'Select' %>	<% else %>		<%= f.collection_select :event_type, LifeEventType.all, :Code, :descr_title, :value => @event.event_type, :prompt => 'Select Event Type' %>	<% end %>  </div><div class="span-8 last" id="new-event"> 	<% if controller_name == 'private_events' && chk_action(action_name, @event) %> 		<span>Frequency </span><%= f.collection_select :reoccurrencetype, ReoccurrenceType.all, :code, :descr_title, :prompt => 'Select' %>	<% end %></div></div><hr />