<div class="span-5 event-label">   	<p><%= f.label "What are you planning?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<span>Name </span><%= f.text_field :event_name, :size => 60 %></div><div class="span-19 last" id="new-event">  <div id="eventtype">	<span>Event Type </span><%= f.collection_select :event_type, get_etype_data(controller_name), :code, :descr_title, :prompt => 'Select' %>  </div>	  <% if controller_name == 'private_events' && chk_action(action_name, @event) %>   	<div class="span-9"> 		<span>Frequency </span><%= f.collection_select :reoccurrencetype, ReoccurrenceType.all, :code, :descr_title, {:prompt => 'Select'}, {:id=>'reoccur-type'} %>  	</div>		  	<div class='span-10 last' id="reoccur" style="<%= reoccurring?(@event) ? '' : 'display:none' %>">		<span>End Date:</span>		<%= f.text_field :reoccurrenceenddate, :value => get_nice_date(@event.reoccurrenceenddate || Date.today, 'Edit'), :size=>10, :id=>'reoccur-dt' %>	</div>	  <% else %>     <span>Annual</span><div class="cbox-top"><%= f.check_box :annualsamedate, {:id => "annual"}, "yes", "no" %></div>  <% end %> </div><hr />