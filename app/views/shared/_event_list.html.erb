  <%= render 'shared/show_import_event' %>
  <table>
  <tr>
    <th>Event Name</th>
    <th>Overview</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Start</th>
    <th>End</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% for event in @events %>
    <tr>
      <td><%= link_to event.event_name, event_url(event, :etype=>event.event_type, :eid=>event.eventid, :sdt=>event.eventstartdate) %></td>
      <td><%= truncate(event.bbody, :length => 40, :omission => "...") %></td>
      <td><%= get_nice_date(event.eventstartdate,'List') %></td>
      <td><%= get_nice_date(event.eventenddate,'List') %></td>
      <td><%= chk_offset(event.eventstarttime, event.localGMToffset, event.eventid) %></td>
      <td><%= chk_offset(event.eventendtime, event.endGMToffset, event.eventid) %></td>
      <% if life_event?(event.event_type) %>
    	<td><%= link_to "Edit", edit_life_event_url(event, :eid=>event.eventid) %></td>
      	<td><%= link_to "Clone", clone_life_event_path(event, :eid=>event.eventid), :confirm => 'Clone this event?' %></td>
      	<td><%= link_to "Remove", life_event_url(event, :eid=>event.eventid, :sdate=>@start_date), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>
  	  <% elsif private_event?(event.event_type) && !tsd_event?(event.eventid[0..1]) %>  
    	<td><%= link_to "Edit", edit_private_event_path(event, :eid=>event.eventid) %></td>
      	<td><%= link_to "Clone", clone_private_event_path(event, :eid=>event.eventid), :confirm => 'Clone this event?' %></td>
      	<td><%= link_to "Remove", private_event_url(event, :eid=>event.eventid, :sdate=>@start_date), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>
	  <% else %>
   		<td></td>
      	<td></td>
      	<td><%= link_to "Remove", scheduled_event_url(event, :eid=>event.eventid, :sdate=>@start_date), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>	  	
      <% end %>
    </tr>
  <% end %>
</table>
<div class="nav-right">
  <%= will_paginate @events, :params => { :controller => "private_events"} %>
</div>
