   <tr>      <td><%= link_to truncate(event.event_name, :length => 35, :omission => "..."), event_url(event, :etype=>event.event_type, :eid=>event.eventid, :sdt=>event.eventstartdate) %></td>      <td><%= truncate(event.bbody, :length => 30, :omission => "...") %></td>      <td><%= get_nice_date(event.eventstartdate,'List') %></td>      <td><%= get_nice_date(event.eventenddate,'List') %></td>      <td><%= chk_offset(event.eventstarttime, event.localGMToffset, event.eventid) %></td>      <td><%= chk_offset(event.eventendtime, event.endGMToffset, event.eventid) %></td>      <% if life_event?(event.event_type) %>    	<td><%= link_to "Edit", edit_life_event_url(event, :eid=>event.eventid) %></td>      	<td><%= link_to "Clone", clone_life_event_path(event, :eid=>event.eventid), :confirm => 'Clone this event?' %></td>      	<td><%= link_to "Remove", life_event_url(event, :eid=>event.eventid, :edate=>event.eventstartdate), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>  	  <% elsif private_event?(event.event_type) && !tsd_event?(event.eventid[0..1]) %>      	<td><%= link_to "Edit", edit_private_event_path(event, :eid=>event.eventid) %></td>      	<td><%= link_to "Clone", clone_private_event_path(event, :eid=>event.eventid), :confirm => 'Clone this event?' %></td>      	<td><%= link_to "Remove", private_event_url(event, :eid=>event.eventid, :edate=>event.eventenddate), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>	  <% else %>   		<td></td><td></td>      	<td><%= link_to "Remove", scheduled_event_url(event, :eid=>event.eventid, :edate=>event.eventenddate), :confirm => 'Remove this event?', :id=>'remove_btn', :method => :delete, :remote=>true %></td>	  	      <% end %>
    </tr>