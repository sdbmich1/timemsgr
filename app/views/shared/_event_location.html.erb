<div class="span-5 event-label">  	<p><%= f.label "Where does it take place?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<span>Place </span><div id="mform"><p>		<% if controller_name == 'private_events' %>			<label for="private_event_mapplacename">		<% elsif controller_name == 'life_events' %>			<label for="life_event_mapplacename">		<% else %>			<label for="scheduled_event_mapplacename">		<% end %>						Enter place name (e.g. Empire State Bldg)</label>		<%= f.text_field :mapplacename, :size => 60 %></p></div>	<div class="span-19 last">		<span>Street </span><%= f.text_field :mapstreet, :size => 60 %>	</div>	<div class="span-8">		<span>City </span><%= f.text_field :mapcity, :size => 25 %>	</div>	<div class="span-5">		<span>State </span><%= f.collection_select :mapstate, StateCode.all, :StateAbbr, :StateAbbr, :include_blank => true %>	</div>	<div class="span-6 last">		<span>Zip/Postal </span><%= f.text_field :mapzip, :size => 10 %>	</div>	<div class="span-19 last">		<span>Country </span><%= f.country_select :mapcountry, ["United States"], :include_blank => true %>	</div></div>
<hr />   