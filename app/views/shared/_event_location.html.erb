<div class="span-5 event-label">  	<p><%= f.label "Where does it take place?" %></p>           </div>	<div class="span-19 last" id="new-event">  	<span>Place </span><div id="mform"><p>			<label for="<%= get_label_type(controller_name) %>_mapplacename">			Enter place name (e.g. Empire State Bldg)</label>		<%= f.text_field :mapplacename, :size => 60 %></p></div> 	<div class="span-19 last">		<span>Street </span><%= f.text_field :mapstreet, :size => 60 %>	</div>	<div class="span-19 last">		<span>Location </span><div id="mform"><p>			<label for="<%= get_label_type(controller_name) %>_location">      		Enter location (e.g. Conf Room A)</label>         <%= f.text_field :location, :size => 60 %></p></div>    </div>	<div class="span-8">		<span>City </span><%= f.text_field :mapcity, :size => 25 %>	</div>	<div class="span-5">		<span>State </span><%= f.collection_select :mapstate, StateCode.all, :StateAbbr, :StateAbbr, :include_blank => true %>	</div>	<div class="span-6 last">		<span>Zip/Postal </span><%= f.text_field :mapzip, :size => 10 %>	</div>	<div class="span-19 last">		<span>Country </span><%= f.country_select :mapcountry, ["United States"], :include_blank => true %>	</div></div>
<hr />   