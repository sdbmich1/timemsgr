<div id="form">
<% if controller_name == 'private_events' %>
	<%= render :partial => 'shared/menu_head', :locals => { :menu_type => 'PrivateEvent' } %>
<% else %>
	<%= render :partial => 'shared/menu_head', :locals => { :menu_type => 'Event' } %>
<% end %>

<%= content_for :left_sidebar do -%>
	<% if set_panel %>
		<div id="user_panel"><%= render set_panel %></div>
	<% end %>
<% end -%>

<%= content_for :content do -%>
	<%= render 'shared/content' %>
<% end -%>
   
<%= content_for :right_sidebar do -%>
	<div id="ad_panel"><%= render 'shared/ad_panel' %></div> 
<% end -%>

<%= content_for :footer do %>
	<%= render 'layouts/footer' %>
<% end %>
</div>