<!-- used to navigate user profile edit modules --><div class="span-2" id="user_sidebar"> 	<ul> 		<li class="all_btn"><%= link_to "All", relationships_path(:id =>@user), :id=>'connect_btn', :remote=>true %></li>		<li class="prv_btn"><%= link_to "Private", private_relationships_path(:id =>@user, :rtype => "Private"), :id=>'connect_btn', :remote=>true %></li>		<li class="soc_btn"><%= link_to "Social", social_relationships_path(:id => @user, :rtype => "Social"), :id=>'connect_btn', :remote=>true %></li>		<li class="ext_btn"><%= link_to "Extended", extended_relationships_path(:id => @user, :rtype => "Extended"), :id=>'connect_btn', :remote=>true %></li>		<li class="pend_btn"><%= link_to "Pending", pending_relationships_path(:id => @user), :id=>'connect_btn', :remote=>true %></li>		<% if @rel_type && controller_name == 'relationships' %>			<li><%= link_to "Add", users_path(:id => @user, :rtype => @rel_type) %></li>		<% end %>	</ul></div>