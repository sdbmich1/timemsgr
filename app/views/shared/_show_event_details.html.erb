<p><div class="prepend-6 span-16 last"><span class="event-title"><%= event.event_title.html_safe %></span></div></p><div class="span-24 last">  <div class="span-4">  	<%= render :partial => 'shared/show_photo', :locals => {:model => event, :psize => '120x120', :file_name=>'schedule1.jpg' }%>  </div>  <div class="span 20 last">	<div class="span-2"><%= 'Date:' %></div>	<div class="span-18 last"><%= get_start_date(event.eventstartdate, event.eventenddate, "Show") %><br /></div>		<div class="span-2"><%= 'Time:'%></div>
	<% if holiday?(event.event_type) %>		<div class="span-18 last"><%= "All Day" %></div>	<% else %>		<div class="span-18 last">			<%= " #{chk_offset(event.eventstarttime, event.localGMToffset, event.eventid)} " if event.eventstarttime %>			<%= " - #{chk_offset(event.eventendtime, event.endGMToffset, event.eventid)}" if event.eventendtime %>			<%= "TBD" unless event.eventstarttime || event.eventendtime %>		</div>	<% end %>	  	<div class="prepend-4">  		<%= render :partial => 'shared/show_event_contact', :locals => {:event => @event}  %>  		<%= render :partial => 'shared/show_event_location', :locals => {:event => @event}  %>  		<%= render :partial => 'shared/show_event_fees', :locals => {:event => @event} %>  	  		<%= render :partial => 'shared/show_event_footer', :locals => {:event => @event} %>  	  	</div>  </div>
</div>