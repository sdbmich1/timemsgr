<br /><div class="span-8" id="new-event"> 	<p>				<div style="display:none"><div id='modal'>        	<div id="notify"><%= render :partial => 'shared/notify', :locals=>{:etype=> event.event_type} %></div>    	</div></div>						<% if rsvp?(event.rsvp) %>			<% if private_event?(event.event_type) %>							<%= link_to "RSVP", rsvp_url(:id=>event.eventid, :eid=>event, :etype=>event.event_type), :class=>"edit-btn round nav-right" %>			<% else %>				<%= link_to "RSVP", new_rsvp_url(:eid=>event, :etype=>event.event_type), :class=>"edit-btn round nav-right" %>						<% end %>	  	<% end %>		</p></div>