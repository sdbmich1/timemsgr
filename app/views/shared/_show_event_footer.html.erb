<br /><div class="span-8 prepend-4" id="new-event"> 	<p>  	<% if life_event?(event.event_type) %> 		<%= button_to "Edit", edit_life_event_url(event), :method => :get %>  	<% elsif private_event?(event.event_type) && chk_action(action_name, event) %>    		<%= button_to "Edit", edit_private_event_url(event), :method => :get %>  	<% elsif private_event?(event.event_type) && !chk_action(action_name, event) %>    		<%= button_to "Edit", edit_scheduled_event_url(event), :method => :get %></p>	<% end %></div>