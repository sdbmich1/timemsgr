	<% if private_event?(event.event_type) || life_event?(event.event_type) %>				<%= render :partial => 'shared/show_notify', :locals => {:event => event} %>	<% end %>	  	<% if life_event?(event.event_type) %>  		<%= link_to "Edit", edit_life_event_url(event, :eid=>event.eventid, :sdate=>event.eventstartdate), :class=>"edit-btn round nav-right" %>		<%= link_to "Delete", life_event_url(event, :eid=>event.eventid, :sdate=>event.eventstartdate), :method => 'delete', :confirm => 'Remove this event?', :class=>"edit-btn round nav-right", :id => "delete" %>  	<% elsif private_event?(event.event_type) %>    		<%= link_to "Edit", edit_private_event_url(event, :eid=>event.eventid, :sdate=>event.eventstartdate), :class=>"edit-btn round nav-right" %>		<%= link_to "Delete", private_event_url(event, :eid=>event.eventid, :sdate=>event.eventstartdate), :method => 'delete',  :confirm => 'Remove this event?', :id => "delete", :class=>"edit-btn round nav-right" %>  	<% else %>		<%= link_to "+ Schedule", private_events_url(:id=>event, :etype=>event.event_type, :eid=>event.eventid, :sdate=>event.eventstartdate), :confirm => 'Add to your schedule?', :class=>"edit-btn round nav-right", 			:method => :post if event.eventenddate >= Date.today %>	<% end %>		<% if rsvp?(event.rsvp) %>		<% if private_event?(event.event_type) %>						<%= link_to "RSVP", rsvp_url(:id=>event.eventid, :eid=>event, :etype=>event.event_type), :id => "rsvp", :class=>"edit-btn round nav-right", :remote=>true %> 		<% else %>			<%= link_to "RSVP", new_rsvp_url(:eid=>event, :etype=>event.event_type, :sdate=>event.eventstartdate), :confirm => 'RSVP for this event?', :id => "rsvp", :class=>"edit-btn round nav-right" %>				<% end %>  	<% end %>  	<%= link_to 'View Site', event.location, :class=>"edit-btn round nav-right" unless (event.location =~ /http/i).nil? %>