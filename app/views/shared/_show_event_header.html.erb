  	<% if life_event?(event.event_type) %>   	  <% unless @user.ssid == event.cid %> 		<%= link_to "Edit", edit_life_event_url(event), :class=>"edit-btn round nav-right" %>		<%= link_to "Delete", life_event_url(event), :method => 'delete', :confirm => 'Are you sure?', :class=>"edit-btn round nav-right", :id => "delete" %>	  <% end %>  	<% elsif private_event?(event.event_type) && chk_action(action_name, event) %>    	  <% unless @user.ssid == event.cid %>  		<%= link_to "Edit", edit_private_event_url(event), :class=>"edit-btn round nav-right" %>		<%= link_to "Delete", private_event_url(event), :method => 'delete',  :confirm => 'Remove this event?', :id => "delete", :class=>"edit-btn round nav-right" %>	  <% end %>  	<% elsif private_event?(event.event_type) && !chk_action(action_name, event) %>    	  <% unless @user.ssid == event.cid %>		<%= link_to "Delete", scheduled_event_url(event), :method => 'delete', :confirm => 'Remove this event?', :id => "delete", :class=>"edit-btn round nav-right" %>	  <% end %>  	<% else %>		<%= link_to "Schedule", scheduled_events_url(:id=>event, :etype=>event.event_type, :sdate=>event.eventstartdate), :confirm => 'Add to your schedule?', :class=>"edit-btn round nav-right", :method => :post %>	<% end %>	<% if private_event?(event.event_type) || life_event?(event.event_type) %>				<%= render :partial => 'shared/show_notify', :locals => {:event => event} %>	<% end %>	<% if rsvp?(event.rsvp) %>		<% if private_event?(event.event_type) %>						<%= link_to "RSVP", rsvp_url(:id=>event.eventid, :eid=>event, :etype=>event.event_type), :id => "rsvp", :class=>"edit-btn round nav-right", :remote=>true %> 		<% else %>			<%= link_to "RSVP", new_rsvp_url(:eid=>event, :etype=>event.event_type, :sdate=>event.eventstartdate), :confirm => 'RSVP for this event?', :id => "rsvp", :class=>"edit-btn round nav-right" %>				<% end %>  	<% end %>