  	<% if life_event?(event.event_type) %>  		<%= link_to "Edit", edit_life_event_url(event), :class=>"edit-btn round nav-right" %>  	<% elsif private_event?(event.event_type) && chk_action(action_name, event) %>    		<%= link_to "Edit", edit_private_event_url(event), :class=>"edit-btn round nav-right" %>  	<% elsif private_event?(event.event_type) && !chk_action(action_name, event) %>    		<%= link_to "Edit", edit_scheduled_event_url(event), :class=>"edit-btn round nav-right" %>  	<% else %>		<%= link_to "Schedule", scheduled_events_url(:id=>event, :etype=>event.event_type, :sdate=>event.eventstartdate), :confirm => 'Add to your schedule?', :class=>"edit-btn round nav-right", :method => :post %>	<% end %>