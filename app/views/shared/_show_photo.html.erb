<% if model %>
  <% for pic in model.pictures %>
  	<% if pic.photo_file_name.blank? %>
		<%= image_tag file_name, :alt => "none", :size => psize %> 
	<% else %>   
		<%= image_tag pic.photo.url, :size => psize %>
	<% end %>
  <% end %>
  
  <% if has_facebook_photo? && model.pictures.blank? %>
	<%= image_tag chk_photo(controller_name, action_name), :alt => "none", :size => psize %> 
  <% elsif !(model.class.to_s =~ /Event/i).nil? && image_exists?(model) %>
   	<%= image_tag model.imagelink, :size => psize rescue nil %>  	
  <% elsif model.pictures.blank? %>
    <%= image_tag file_name, :alt => "none", :size => psize %>
  <% end %>
<% end %>
