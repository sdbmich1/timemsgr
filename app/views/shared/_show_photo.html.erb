  <% for pic in model.pictures %>
      <%= image_tag pic.photo.url, :size => psize %>
  <% end %>
  <% if has_facebook_photo? && controller_name == 'events' %>
   	<%= image_tag @facebook_user.picture, :size => psize %>   
  <% elsif model.try(:pictures).blank? %>
    <%= image_tag file_name, :alt => "none", :size => psize %>
  <% end %>