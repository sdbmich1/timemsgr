<!-- used to navigate user profile edit modules --><div class="span-4" id="user_sidebar">   <ul class="menu collapsible">   <% @categories.each do |category| %>	<li class="span-4">		<%= link_to category.name, "#", {:class=>"category_menu", "data-catid"=>category.id, :id=>'catlist_btn'} %>		<ul class="acitem">			<% category.interests.reorder('name').each do |interest| %>				<li class="span-4"><%= link_to interest.name, "#", {:class=>"channel_menu", "data-intid"=>interest.id, :id=>'chlist_btn'} %></li>			<% end %>		</ul>	</li>  <% end %>  </ul></div>