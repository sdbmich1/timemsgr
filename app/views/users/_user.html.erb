<%= render 'shared/edit_form' %> <div class="span-20 last round outer-shadow">   	<%= form_for setup_user(@user), :html => { :multipart => true }, :validate => true do |f| %>  		<div class="span-16 edit-usr pg-title" id="new-event">  			<%= render "shared/error_messages", :target => @user %>  			<%= render :partial => set_area, :locals => { :f => f } %> 		</div> 		<div class="span-4 last pg-title">			<%= render :partial => 'shared/show_photo', :locals => {:model=>@user.profile, :psize=>"80x80", :file_name=>"#{showphoto(@user.gender)}"}%> 		</div>		<p><%= f.submit "Save" %></p>	<% end %>  </div> 