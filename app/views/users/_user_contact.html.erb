<%= f.fields_for :host_profiles do |form| %>  	<%= render :partial => 'shared/user_address', :locals => {:form => form, :toggle => true} %>	<p><span>Work Phone </span><%= form.text_field :Phone_Work %></p>	<p><span>Cell Phone </span><%= form.text_field :Phone_cell %></p>	<p><span>Wireless Provider </span>		<%= form.collection_select :wirelessservice, WirelessProvider.unhidden, :code, :description, {:prompt => 'Select Provider'} %></p><% end %> 