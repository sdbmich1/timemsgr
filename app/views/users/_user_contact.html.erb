<%= f.fields_for :host_profiles do |form| %>  	<br /><p><span>Home Phone </span><%= form.text_field :home_phone %></p>	<p><span>Work Phone </span><%= form.text_field :work_phone %></p>	<p><span>Cell Phone </span><%= form.text_field :cell_phone %></p>	<p><span>Address 1 </span><%= form.text_field :address1 %></p>	<p><span>Address 2 </span><%= form.text_field :address2 %></p>	<p><span>City </span><%= form.text_field :city %></p>	<p><span>State/Province </span>		<%= form.collection_select :state, StateCode.all, :StateAbbr, :State, :prompt => true %></p>	<p><span>Zip </span><%= form.text_field :postalcode, :size => 10 %></p>	<p><span>Country </span><%= form.country_select :country, ["United States"], :prompt => true %></p>	  <% end %> 