<%= set_title("User List") %>
<%= render :partial => 'shared/menu_head', :locals => { :menu_type => 'User' } %>

<% content_for :subhead do -%>
  <center><h3 class="grp-hdr">Suggested Connections</h3></center><hr />
<% end %>

<% content_for :left_sidebar do -%>
	<%= render :partial => 'shared/rel_nav_panel' %>
<% end %>

<%= content_for :content do -%>
  <div class="span-20 last"> 
  	<div class="prepend-1">
	<% @subscriptions.each do |sub| %>
		<% unless sub.try(:channel).blank? %>
			<div id="user_form"><%= render :partial => 'shared/user_list', :locals => {:model => sub.channel.subscriptions }%></div>
		<% end %>
	<% end %>
	</div>
  </div>
<% end %>
